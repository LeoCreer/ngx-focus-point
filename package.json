{
  "name": "ngx-focus-point",
  "version": "15.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/LeoCreer/ngx-focus-point"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --configuration production --output-path=www --base-href=/ngx-focus-point/",
    "build-library": "ng build ngx-focus-point --configuration production",
    "publish-library": "cd dist/ngx-focus-point && npm publish",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "dev:ssr": "ng run ngx-focus-point:serve-ssr",
    "serve:ssr": "node dist/ngx-focus-point/server/main.js",
    "build:ssr": "ng build --configuration production && ng run ngx-focus-point:server:production",
    "build:ssr:staging": "ng build --configuration staging && ng run ngx-focus-point:server",
    "prerender": "ng run ngx-focus-point:prerender"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.0.0",
    "@angular/common": "^15.0.0",
    "@angular/compiler": "^15.0.0",
    "@angular/core": "^15.0.0",
    "@angular/forms": "^15.0.0",
    "@angular/platform-browser": "^15.0.0",
    "@angular/platform-browser-dynamic": "^15.0.0",
    "@angular/platform-server": "^15.0.0",
    "@angular/router": "^15.0.0",
    "@nguniversal/express-engine": "^15.0.0",
    "core-js": "^3.6.5",
    "domino": "^2.1.6",
    "express": "^4.15.2",
    "rxjs": "^7.5.7",
    "tslib": "^2.3.1",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.0.0",
    "@angular-eslint/builder": "15.0.0",
    "@angular-eslint/eslint-plugin": "15.0.0",
    "@angular-eslint/eslint-plugin-template": "15.0.0",
    "@angular-eslint/schematics": "15.0.0",
    "@angular-eslint/template-parser": "~15.0.0",
    "@angular/cli": "^15.0.0",
    "@angular/compiler-cli": "^15.0.0",
    "@angular/language-service": "^15.0.0",
    "@nguniversal/builders": "^15.0.0",
    "@types/express": "^4.17.0",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.8",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.43.0",
    "codelyzer": "^6.0.0",
    "eslint": "^8.28.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.4",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^15.0.0",
    "prettier": "2.0.5",
    "protractor": "~7.0.0",
    "ts-node": "~8.9.1",
    "tslint": "~6.1.0",
    "typescript": "~4.8.4"
  }
}
