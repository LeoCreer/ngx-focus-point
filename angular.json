{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "ngx-focus-point": {
      "root": "projects/ngx-focus-point",
      "sourceRoot": "projects/ngx-focus-point/src",
      "projectType": "library",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "tsConfig": "projects/ngx-focus-point/tsconfig.lib.json",
            "project": "projects/ngx-focus-point/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/ngx-focus-point/tsconfig.lib.prod.json"
            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/ngx-focus-point/src/test.ts",
            "tsConfig": "projects/ngx-focus-point/tsconfig.spec.json",
            "karmaConfig": "projects/ngx-focus-point/karma.conf.js"
          }
        }
      }
    },
    "ngx-focus-point-e2e": {
      "root": "e2e/",
      "projectType": "application",
      "prefix": "",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "ngx-focus-point:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "ngx-focus-point:serve:production"
            }
          }
        }
      }
    }
  },
  "cli": {
    "analytics": "bb16341c-1df5-449c-8a3a-7990f89f4092"
  },
  "schematics": {
    "@angular-eslint/schematics:application": {
      "setParserOptionsProject": true
    },
    "@angular-eslint/schematics:library": {
      "setParserOptionsProject": true
    }
  }
}
